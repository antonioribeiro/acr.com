{
    "name": "laravel/laravel",
    
    "description": "The Laravel Framework.",

    "keywords": ["framework", "laravel"],

    "license": "MIT",

    "require": {
        "laravel/framework": "4.2.*",
        "doctrine/dbal": "dev-master",
        "cartalyst/sentry": "dev-master",
        "cartalyst/sentinel": "dev-master",
        "way/generators": "dev-master",
        "onigoetz/profiler": "dev-master",
        "way/form": "dev-master",
        "michelf/php-markdown": "1.4.*@dev",
        "pragmarx/glottos": "dev-master",
        "pragmarx/firewall": "dev-master",
        "pragmarx/deeployer": "dev-master",
        "pragmarx/steroids": "dev-master",
        "pragmarx/tracker": "dev-master",
        "pragmarx/helpers": "dev-master",
        "pragmarx/sqli": "0.*",
        "itsgoingd/clockwork": "1.*",
        "intervention/image": "1.*",
        "geoip/geoip": "~1.14",
        "jenssegers/agent": "*"
    },

    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/antonioribeiro/tracker.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/antonioribeiro/helpers.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/cartalyst/sentinel.git"
        }
    ],

    "autoload": {
        "classmap": [
            "app/commands",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ],
        "psr-0": {
            "ACR": "app/acr.com"
        },
        "psr-4": {
            "App\\": "app/App"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },


    "config": {
        "preferred-install": "dist"
    },
    
    "minimum-stability": "dev",
    "require-dev": {
    }
}
